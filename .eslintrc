{
    "env": {
      "es6": true,
      "browser": true
    },
    "extends": "eslint:recommended",
    "globals": {
      "require": true,
      "module": true,
      "$": true
    },
    "rules": {
      "no-console": 0,
      "no-global-assign": [
        "error",
        {
          "exceptions": ["Object"]
        }
      ],
      "indent": 0,
      "linebreak-style": ["error", "unix"],
      "quotes": ["warn", "single"],
      "semi": ["warn", "always"]
    }
  }